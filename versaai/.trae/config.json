  "custom_commands": {
    "start_dev": {
      "description": "Iniciar entorno de desarrollo completo",
      "command": "docker-compose up -d postgres redis && npm run dev --prefix frontend & uvicorn src.main:app --reload",
      "parallel": true
    },
    "run_tests": {
      "description": "Ejecutar todos los tests",
      "command": "pytest tests/ -v --cov=src && npm run test --prefix frontend",
      "parallel": false
    },
    "format_code": {
      "description": "Formatear todo el c贸digo",
      "command": "black src/ && isort src/ && npm run lint:fix --prefix frontend",
      "parallel": false
    },
    "build_prod": {
      "description": "Build para producci贸n",
      "command": "npm run build --prefix frontend && docker-compose -f docker-compose.prod.yml build",
      "parallel": false
    },
    "debug_auth": {
      "description": "Depurar problemas de autenticaci贸n",
      "command": "python debug_user_creation.py && python test_auth_endpoints.py",
      "parallel": false
    },
    "reset_db": {
      "description": "Resetear base de datos de desarrollo",
      "command": "alembic downgrade base && alembic upgrade head",
      "parallel": false
    },
    "generate_migration": {
      "description": "Generar nueva migraci贸n",
      "command": "alembic revision --autogenerate -m",
      "parallel": false,
      "requires_input": true
    },
    "check_health": {
      "description": "Verificar salud del sistema",
      "command": "python check_db.py && curl -f http://localhost:8000/api/health",
      "parallel": false
    },
    "optimize_imports": {
      "description": "Optimizar imports en todo el proyecto",
      "command": "isort src/ tests/ && autoflake --remove-all-unused-imports --recursive src/ tests/",
      "parallel": false
    }
  },
  
  "file_templates": {
    "fastapi_router": {
      "path": "src/routers/{name}.py",
      "template": "fastapi-crud",
      "auto_imports": ["FastAPI", "HTTPException", "Depends", "status"]
    },
    "vue_component": {
      "path": "frontend/src/components/{name}.vue",
      "template": "vue-component",
      "auto_imports": ["ref", "computed", "onMounted"]
    },
    "pydantic_schema": {
      "path": "src/schemas/{name}.py",
      "template": "pydantic-schema",
      "auto_imports": ["BaseModel", "Field", "validator"]
    },
    "sqlalchemy_model": {
      "path": "src/models/{name}.py",
      "template": "sqlmodel",
      "auto_imports": ["SQLModel", "Field", "Relationship"]
    },
    "service_layer": {
      "path": "src/services/{name}_service.py",
      "template": "service-layer",
      "auto_imports": ["Depends", "get_db", "HTTPException"]
    },
    "pytest_test": {
      "path": "tests/test_{name}.py",
      "template": "pytest-test",
      "auto_imports": ["pytest", "TestClient", "override_get_db"]
    },
    "vue_composable": {
      "path": "frontend/src/composables/use{name}.ts",
      "template": "vue-composable",
      "auto_imports": ["ref", "computed", "reactive"]
    },
    "pinia_store": {
      "path": "frontend/src/stores/{name}.ts",
      "template": "pinia-store",
      "auto_imports": ["defineStore", "ref", "computed"]
    },
    "api_client": {
      "path": "frontend/src/services/{name}Api.ts",
      "template": "api-client",
      "auto_imports": ["apiClient", "ApiResponse"]
    },
    "middleware": {
      "path": "src/middleware/{name}.py",
      "template": "fastapi-middleware",
      "auto_imports": ["Request", "Response", "BaseHTTPMiddleware"]
    },
    "alembic_migration": {
      "path": "alembic/versions/{timestamp}_{name}.py",
      "template": "alembic-migration",
      "auto_imports": ["alembic", "sqlalchemy"]
    }
  },
  
