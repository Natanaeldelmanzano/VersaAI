{
  "name": "versaai-frontend",
  "version": "2.0.0",
  "description": "VersaAI Frontend - Enterprise AI Chatbot Platform",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "node build.js",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "preview:prod": "vite preview --port 4173 --host",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "type-check": "vue-tsc --noEmit",
    "clean": "rimraf dist",
    "serve": "npm run build && npm run preview",
    "deploy:staging": "npm run build:prod && echo 'Ready for staging deployment'",
    "deploy:prod": "npm run build:prod && echo 'Ready for production deployment'"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@vueuse/core": "^10.5.0",
    "axios": "^1.6.2",
    "chart.js": "^4.5.0",
    "date-fns": "^2.30.0",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "lucide-vue-next": "^0.525.0",
    "marked": "^9.1.6",
    "pinia": "^2.1.7",
    "prismjs": "^1.29.0",
    "vue": "^3.3.8",
    "vue-chartjs": "^5.2.0",
    "vue-router": "^4.2.5",
    "vue-toastification": "^2.0.0-rc.5",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash": "^4.14.202",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vue/test-utils": "^2.4.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.54.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^22.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "tailwindcss": "^3.4.17",
    "terser": "^5.26.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.5",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.25"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "keywords": [
    "vue",
    "vite",
    "pwa",
    "dashboard",
    "ai",
    "chatbot",
    "enterprise",
    "analytics"
  ],
  "author": "VersaAI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/versaai/dashboard-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/versaai/dashboard-frontend/issues"
  },
  "homepage": "https://dashboard.versaai.com"
}
