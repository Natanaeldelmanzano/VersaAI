# Configuración de Producción - VersaAI Dashboard

# API Configuration
VITE_API_URL=https://api.versaai.com
VITE_API_VERSION=v1
VITE_API_TIMEOUT=30000

# App Configuration
VITE_APP_NAME=VersaAI Enterprise Dashboard
VITE_APP_VERSION=2.0.0
VITE_APP_DESCRIPTION=Enterprise AI Dashboard for chatbot management and analytics

# Environment
NODE_ENV=production
VITE_ENV=production

# Features Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_MOBILE=true
VITE_ENABLE_CUSTOMIZATION=true
VITE_ENABLE_ADVANCED_ANALYTICS=true

# Security
VITE_ENABLE_CSP=true
VITE_ENABLE_HTTPS_ONLY=true

# Performance
VITE_ENABLE_COMPRESSION=true
VITE_ENABLE_CACHING=true
VITE_CACHE_DURATION=86400

# Monitoring
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Social/External
VITE_SUPPORT_EMAIL=support@versaai.com
VITE_COMPANY_URL=https://versaai.com
VITE_DOCS_URL=https://docs.versaai.com

# CDN Configuration
VITE_CDN_URL=https://cdn.versaai.com
VITE_ASSETS_URL=https://assets.versaai.com

# WebSocket Configuration
VITE_WS_URL=wss://ws.versaai.com
VITE_WS_RECONNECT_INTERVAL=5000

# Analytics
VITE_GOOGLE_ANALYTICS_ID=
VITE_MIXPANEL_TOKEN=

# Sentry (Error Tracking)
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=production

# Feature Limits
VITE_MAX_FILE_SIZE=10485760
VITE_MAX_CHATBOTS=100
VITE_MAX_CONVERSATIONS=10000

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true
VITE_DEFAULT_LANGUAGE=es

# PWA Configuration
VITE_PWA_CACHE_NAME=versaai-cache-v1
VITE_PWA_OFFLINE_PAGE=/offline.html

# Build Configuration
VITE_BUILD_TIMESTAMP=__BUILD_TIMESTAMP__
VITE_BUILD_VERSION=__BUILD_VERSION__
VITE_GIT_COMMIT=__GIT_COMMIT__