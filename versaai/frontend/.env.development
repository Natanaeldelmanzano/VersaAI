# Configuraci√≥n de Desarrollo - VersaAI Dashboard

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_API_VERSION=v1
VITE_API_TIMEOUT=10000

# App Configuration
VITE_APP_NAME=VersaAI Enterprise Dashboard (Dev)
VITE_APP_VERSION=2.0.0-dev
VITE_APP_DESCRIPTION=Enterprise AI Dashboard for chatbot management and analytics - Development

# Environment
NODE_ENV=development
VITE_ENV=development

# Features Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_MOBILE=true
VITE_ENABLE_CUSTOMIZATION=true
VITE_ENABLE_ADVANCED_ANALYTICS=true

# Development Features
VITE_ENABLE_DEV_TOOLS=true
VITE_ENABLE_HOT_RELOAD=true
VITE_ENABLE_SOURCE_MAPS=true

# Security (Relaxed for development)
VITE_ENABLE_CSP=false
VITE_ENABLE_HTTPS_ONLY=false

# Performance (Disabled for development)
VITE_ENABLE_COMPRESSION=false
VITE_ENABLE_CACHING=false
VITE_CACHE_DURATION=0

# Monitoring (Disabled for development)
VITE_ENABLE_ERROR_TRACKING=false
VITE_ENABLE_PERFORMANCE_MONITORING=false

# Social/External
VITE_SUPPORT_EMAIL=dev@versaai.com
VITE_COMPANY_URL=http://localhost:3000
VITE_DOCS_URL=http://localhost:3001

# Local Development URLs
VITE_CDN_URL=http://localhost:3000
VITE_ASSETS_URL=http://localhost:3000

# WebSocket Configuration
VITE_WS_URL=ws://localhost:8001
VITE_WS_RECONNECT_INTERVAL=3000

# Analytics (Disabled for development)
VITE_GOOGLE_ANALYTICS_ID=
VITE_MIXPANEL_TOKEN=

# Sentry (Disabled for development)
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=development

# Feature Limits (Relaxed for development)
VITE_MAX_FILE_SIZE=52428800
VITE_MAX_CHATBOTS=1000
VITE_MAX_CONVERSATIONS=100000

# UI Configuration
VITE_DEFAULT_THEME=light
VITE_ENABLE_DARK_MODE=true
VITE_DEFAULT_LANGUAGE=es

# PWA Configuration
VITE_PWA_CACHE_NAME=versaai-dev-cache-v1
VITE_PWA_OFFLINE_PAGE=/offline.html

# Development Debugging
VITE_DEBUG_MODE=true
VITE_VERBOSE_LOGGING=true
VITE_MOCK_API=false

# Build Configuration
VITE_BUILD_TIMESTAMP=dev
VITE_BUILD_VERSION=dev
VITE_GIT_COMMIT=dev